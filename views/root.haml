- @result.each do |i|
  - unless i[1] == ""
    %div{:class=>"article"}
      %a{:href=>i[3], :style=>"text-decoration: none;"}
        %h2= i[1]
        %p{:style=>"color:black; float: right;"}= i[0]
        %p{:style=>"color:black;"}= i[2]
        - unless i[4] == ""
          %img{:src=>i[4], :style=>"max-height:500px; max-width:500px;"}
